<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script>
  var marketing = "marketing";
  var content = "content";
  var design = "design";
  $.get('/api/'+{{major}}, function(data){
    for(var i=0 ; i<data.length ; i++){
      var row = []
      row.push(i+1);
      row.push(data[i].profile.nickname);
      row.push(data[i].profile.sex);
      row.push(data[i].profile.size);
      row.push(data[i].profile.school);
      row.push(data[i].profile.major);
      for(var j=0 ; j<5 ; j++)
        row.push(data[i].jwcinfo.generalquestion.answers[j].answer)
      for(var k=1; k< data[i].jwcinfo.specialquestion.answers.length; k++)
        row.push(data[i].jwcinfo.specialquestion.answers[k].answer)
      var p = "<tr>";
      for(d of row)
        p+="<td>"+d+"</td>"
      p+="</tr>"
      $("table").append(p);
    }
  });
</script>
